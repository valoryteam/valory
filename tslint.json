{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
	},
	"rules": {
		"indent": true,
		"no-var-requires": false,
		"no-console": [false],
		"no-string-literal": false,
		"align": false,
		"interface-name": ["never-prefix"],
		"object-literal-sort-keys": false,
		"ordered-imports": [false],
		"no-trailing-whitespace": false
	},
	"rulesDirectory": []
}